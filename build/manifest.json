{
  "manifest_version": 2,
  "name": "react-lucid",
  "description": "A devtool for react developers.",
  "version": "1.0",
  "permissions": [
    "*://*/*",
    "tabs",
    "http://*/*",
    "https://*/*",
    "file:///*"
  ],
  "devtools_page": "devtools.html",
  "homepage_url": "https://github.com/Gossamer-React/React-Lucid",
  "content_scripts": [{
    "run_at": "document_end",
    "matches": ["<all_urls>"],
    "js": ["contentScript.js"]
  }],
  "background": {
    "scripts": [
      "backgroundScript.js"
    ]
  },
  "web_accessible_resources": [
    "reactTraverser.js"
  ],
  "externally_connectable": {
    "ids": ["*"]
  }
}